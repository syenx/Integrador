@section scripts{
    <script src="@Url.Content("~/Scripts/perfil.js")"></script>
}
<ol class="breadcrumb">
    <li><a href="@Url.Action("Index","Home")">Home</a></li>
    <li><a href="@Url.Action("Index","Perfil")">Perfil</a></li>
    <li><a href="#">Consulta</a></li>
</ol>
<fieldset>
    <legend>@ViewBag.Title</legend>
     <!-- PESQUISA -->
    <div class="form-inline well">
        <!--Perfil-->
        <label for="busca">Perfil :</label>
        <input name="termo" id="termo" type="text" class="form-control width750" maxlength="50" />
        <!--ATIVO-->
        <input type="radio" name="ativolistar" id="ativolistar" value="true" checked="checked">ATIVO
        <!--INATIVO-->
        <input type="radio" name="ativolistar" id="inativolistar" value="false"> INATIVO
        <a href="#" id="buscaPerfil" name="buscaPerfil" title="Buscar Perfil" class="btn btn-info btn-md col-sm-offset-1"><i class="fa fa-search"></i> Buscar</a>
        <a href="#" id="limparPerfil" name="limparPerfil" title="Limpar Perfil" class="btn btn-danger btn-md"><i class="fa fa-trash"></i> Limpar</a>
    </div>
    <!--Nova Matrícula-->
    <a href="@Url.Action("Create","Perfil")" class="btn btn-info btn-md spacebutton"><i class="fa fa-user-secret fa-fw"></i> Novo Perfil</a>
    <!--Partial View Lista de Perfil Paginas -->
    <div id="listaPerfil"></div>
</fieldset>