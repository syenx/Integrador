@model IEnumerable<ProIntegracao.Data.EntidadeProSimulador.Pessoa>
<table class="table table-hover" cellspacing="0" id="AlunosDuplicados">
    <thead>
        <tr>
            <th class="text-left">CFC</th>
            <th class="text-left">RAZÃO SOCIAL</th>
            <th class="text-left">CPF</th>
            <th class="text-left">NOME ALUNO</th>
            <th class="text-left">TIPO</th>
            <th class="text-left">STATUS CADASTRO</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.Count() > 0)
        {
            foreach (var item in Model)
            {
                long cpf = (string.IsNullOrEmpty(item.CPF)) ? 0 : Convert.ToInt64(item.CPF);
                var docCpf = String.Format(@"{0:000\.000\.000\-00}", cpf);

                <tr>
                    <td>@item.CFC</td>
                    <td>@item.RazaoSocial</td>
                    <td>@docCpf</td>
                    <td>@item.Nome</td>
                    <td>@item.Tipo</td>
                    <td>@item.Status</td>
                </tr>
            }
        }
    </tbody>
</table>
