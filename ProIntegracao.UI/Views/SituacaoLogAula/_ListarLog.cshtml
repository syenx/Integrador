@model IEnumerable<ProIntegracao.Data.Entidade.LogAula>
<table class="table table-hover dt-responsive wrap" cellspacing="0" width="200px" id="tblLogAula">
    <thead>
        <tr>
            <th>ID EMPRESA</th>
            <th>EMPRESA</th>
            <th>DATA CADASTRO</th>
            <th>DETALHES</th>
            <th>IP</th>
            <th></th>
        </tr>
    </thead>

    @foreach (var item in Model)
    {

        var entrada = (item.XmlEntrada.Length > 50) ? item.XmlEntrada.Substring(0, 50) : item.XmlEntrada;
        var saida = (item.XmlSaida.Length > 50) ? item.XmlSaida.Substring(0, 50) : item.XmlSaida;

        <tr>
            <td>@item.IDEmpresa</td>
            <td>@item.Empresa</td>
            <td>@item.DataCadastro.ToString("dd/mm/yyyy")</td>
            <td>@item.Detalhes</td>
            <td>@item.IP</td>
           
            <td>
                <a href="#" onclick="AbreXMLDetalhe('@item.XmlEntrada', '@item.XmlSaida')">
                    <i class="fa fa-search"></i>
                </a>
            </td>
        </tr>
    }
</table>


