<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title - ProIntegracao</title>
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/autocomplete.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />

    @RenderSection("style", false)

    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <!-- Part 1: Wrap all page content here -->
    <div id="wrap">
        <img src="@Url.Content("~/Content/images/bg-banner-sistema.jpg")" class="img-responsive" />

        <nav class="navbar navbar-default">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="/Login/Index"><img class="img-responsive" src="~/img/logo (1).png" alt=""></a>
            </div>

            <div id="navbar" class="navbar-collapse collapse">
                
            </div><!--/.nav-collapse -->
        </nav>

        <!-- Begin page content -->
        <div class="container">
            
            @RenderBody()
        </div>

        <div id="push"></div>
    </div>

    <div id="footer">
        <div class="container">
            <p class="muted credit">&copy; @DateTime.Now.Year - PROINTEGRAÇÃO</p>
        </div>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/waitmodal.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>